                regex_validation : '^[a-zA-Z0-9_]+$',
                validation_message : '@@form.subform.invalidId@@'
            },
            {
                name:'label',
                label:'@@form.subform.label@@',
                type:'textfield',
                value : '@@form.statusTrail.label.value@@'
            },
            {
                name : 'formDefId',
